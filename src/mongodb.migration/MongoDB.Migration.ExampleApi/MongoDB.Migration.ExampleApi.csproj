    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>eec5d7df-9947-4099-ac4c-806c8f4a9025</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>..\..\..</DockerfileContext>
